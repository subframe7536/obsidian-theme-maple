{
  "name": "maple",
  "version": "0.8.34",
  "type": "module",
  "author": "subframe7536",
  "description": "elegant theme for Obsidian",
  "scripts": {
    "dev": "bun scripts/compile.ts src/dev.scss",
    "dev:test": "bun scripts/compile.ts src/snippets/test.scss",
    "build": "bun scripts/compile.ts --build && bun scripts/move.ts",
    "replace-version": "bun scripts/version.ts && bun run build",
    "release": "bumpp -x \"npm run replace-version\" --all"
  },
  "keywords": [
    "Obsidian"
  ],
  "license": "MIT",
  "devDependencies": {
    "bumpp": "^10.2.3",
    "bun-types": "^1.2.21",
    "sass": "^1.91.0"
  }
}
